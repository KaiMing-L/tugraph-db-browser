  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://github.com/TuGraph-family/tugraph-db/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "client-bootstrap": "cd client && yarn install --frozen-lockfile",
    "server-bootstrap": "cd server && yarn install --frozen-lockfile",
    "bootstrap": "yarn run client-bootstrap && yarn run server-bootstrap",
    "client-dev": "cd client && yarn run dev",
    "server-dev": "cd server && yarn run dev",
    "client-start": "cd client && yarn run start",
    "server-start": "cd server && yarn run start",
    "start": "yarn run server-start && yarn run client-dev",
    "client-build": "cd client && yarn run build",
    "server-build": "cd server && yarn run build",
    "build": "npm run client-build && npm run server-build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TuGraph-family"
  }
}
